#degree converter

def c_to_f(temp):
  result = temp * 9/5 + 32 
  print(f'{result}F')

def f_to_c(temp):
  result = (temp - 32) * 5/9
  print(f'{result}C')

degree = str(input("what degree would you like to convert to? F or C?"))
if degree == "F":
  temp = int(input("what is your current temp"))
  c_to_f(temp)
  
  
else:
  degree = "C"
  temp = int(input("what is your current temp"))
  f_to_c(temp)
  


